{
    "name": "wordcount",
    "displayName": "WordCount",
    "description": "VS Code Extension that gets the current word count of the text ediotr.",
    "version": "0.0.1",
    "publisher": "zbennett10",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.wordCount"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [{
            "command": "extension.wordCount",
            "title": "get word count"
        }]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "eslint": "^3.6.0",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}